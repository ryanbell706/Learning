Score = input("Enter Score: ")
s = float(Score)

if s < 0 or s > 1:
    print("Error, Invalid")
elif s > 0.9:
    print("A")
else:
    if s > 0.8:
        print("B")
    elif s > 0.7:
        print("C")
    else:
        if s > 0.6:
            print("D")
        elif s < 0.6:
            print("F")
